{
    "name": "soy-fun",
    "description": "Soy Lang Server",
    "author": "maurinator",
    "license": "MIT",
    "version": "0.0.1",
    "publisher": "maurinator",
    "repository": {
        "type": "git",
        "url": "https://github.com/maurinator/soy-fun"
    },
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:soy"
    ],
    "main": "./build/extension/app.js",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "configuration",
            "properties": {}
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "compile": "rm -rf build && mkdir build && tsc -p ./",
        "package": "vsce package",
        "vscode:prepublish": "npm run compile"
    },
    "dependencies": {
        "glob": "^7.1.2",
        "vscode": "^1.1.17",
        "vscode-languageserver": "^4.1.2",
        "vscode-languageclient": "^4.1.3"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.48",
        "@types/node": "^6.0.101",
        "typescript": "^2.7.2"
    }
}
